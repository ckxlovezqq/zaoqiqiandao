.xui-cell_access {
  color: inherit;
}

.xui-cell__ft_in-access {
  position: relative;
  padding-right: 21px;
}

.xui-cell__ft_in-access:after {
  content: " ";
  position: absolute;
  top: 50%;
  right: 5px;
  display: inline-block;
  height: 5px;
  width: 5px;
  margin-top: -4px;
  border-width: 1px 1px 0 0;
  border-color: #ccc;
  border-style: solid;
  -webkit-transform: matrix(1, 1, -1, 1, 0, 0);
  transform: matrix(1, 1, -1, 1, 0, 0);
}

