}

.xui-cell:before {
  content: " ";
  position: absolute;
  left: 9px;
  top: 0;
  right: 9px;
  height: 1px;
  border-top: 1px solid #e4e4e4;
  color: #d9d9d9;
}

.xui-cell:first-child:before {
  display: none;
}

.xui-cell_active {
  background-color: #ececec;
}

.xui-cell_primary {
  -webkit-align-items: flex-start;
  align-items: flex-start;
}

