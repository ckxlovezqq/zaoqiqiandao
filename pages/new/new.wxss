.switch-checkbox {
    position: relative;
    width: 36px;
    height: 21px;
    background-color: #1DACA9;
    border-radius: 14px;
    box-shadow: inset 1px 1px 1px rgba(0, 0, 0, .1);
    -webkit-transform: scale(1.3);
    transform: scale(1.3);
}

.switch-checkbox::before {
    position: absolute;
    content: '';
    left: -2px;
    top: 0;
    z-index: 2;
    width: 21px;
    height: 21px;
    background-color: #fff;
    border-radius: 50%;
    box-sizing: border-box;
    box-shadow: -1px 1px 2px #777;
    transition: .3s all linear;
}

.switch-checkbox::after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
    width: 36px;
    height: 21px;
    background-color: #fff;
    border-radius: 14px;
    border: 1px solid #dfdfdf;
    box-sizing: border-box;
    transition: .3s all linear;
}

.switch-checkbox.active::after {
    -webkit-transform: scale(0);
    transform: scale(0);
}

.switch-checkbox.active::before {
    box-shadow: 1px 1px 2px #1DACA9;
    -webkit-transform: translate(19px);
    transform: translate(19px);
}

