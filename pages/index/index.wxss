.banner-packing {
        position: relative;
        display: -webkit-flex;
        display: flex;
        -webkit-align-items: center;
        align-items: center;
        -webkit-justify-content: space-between;
        justify-content: space-between;
        -webkit-flex-direction: column;
        flex-direction: column;
        width: 100%;
        height: 200px;
        box-sizing: border-box;
    }

.banner-packing .bd {
    text-align: center;
    color: #fff;
}

.banner-packing .avatar {
    margin-top: 39px;
    width: 67px;
    height: 67px;
    border: 1px solid rgba(255, 255, 255, .7);
    border-radius: 50%;
}

.banner-packing .name {
    display: flex;
    margin-top: 110px;
    margin-left: 90px;
    font-size: 17px;
    line-height: 24px;
    color: #fff;
    text-shadow: 0 1px 1px rgba(0, 0, 0, 0.50);
}

.login-ing-btn {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-align-items: center;
    align-items: center;
    width: 86px;
    height: 28px;
    margin-top: 9px;
    background: #1DACA9;
    border-radius: 2px;
    font-size: 13px;
}

.banner-packing .bg {
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}

.packing-title {
    position: relative;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-align-items: center;
    align-items: center;
}

.packing-title .title {
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    font-size: 15px;
    color: #333;
}

.packing-title .title .line {
    display: inline-block;
    width: 2px;
    height: 14px;
    background-color: #1DACA9;
    margin-right: 7px;
}

.habit_recommend_box {
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    margin-bottom: 14px;
}

.habit_recommend_box:last-child {
    margin-bottom: 0;
}

.habit_recommend_box__hd {
    display: -webkit-flex;
    display: flex;
    margin-right: 9px;
}

.habit_recommend_box__hd .banner {
    width: 115px;
    height: 76px;
}

.habit_recommend_box__bd {
    width: 217px;
}

.habit_recommend_box__bd .title {
    font-size: 15px;
    line-height: 19px;
    color: #333333;
}

.habit_recommend_box__bd .author {
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    font-size: 11px;
}

.habit_recommend_box__bd .author-hd {
    margin-right: 7px;
    width: 19px;
    height: 19px;
    border-radius: 50%;
}

.habit_recommend_box__bd .personal {
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    margin-top: 4px;
    font-size: 11px;
    line-height: 15px;
    color: #999;
}

.habit_recommend_box__bd .personal .line {
    display: inline-block;
    margin: 0 9px;
    width: 1px;
    height: 9px;
    background-color: #e4e4e4;
}

