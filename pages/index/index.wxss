/* 主体 */

wx-button {
    padding: 0;
    margin: 0;
    border-radius: 0;
    text-align: inherit;
    font-size: inherit;
    color: inherit;
    background-color: transparent;
    overflow: visible;
}

wx-button:after {
 
    display: none !important;
}

